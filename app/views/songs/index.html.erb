<table>
  <thead>
    <tr>
      <th>Song</th>
      <th>Artist</th>
      <th>Genre</th>
      <th>Release Year</th>
    </tr>
   </thead>
   <tbody>
    <%@songs.each do |song|%>
    <tr>
    <td><%=song.title%></td>
    <td><%=song.artist_name%></td>
    <td><%=song.genre%></td>
    <td><%=song.release_year if song.released%></td>
    </tr>
  <%end%>
  </tbody>
</table>